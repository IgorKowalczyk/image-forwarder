{
 "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
 "tasks": {
  "dev": "deno run --watch --allow-net main.ts",
  "start": "deno run --allow-net main.ts",
  "fmt": "deno fmt",
  "fmt:check": "deno fmt --check",
  "lint": "deno lint"
 },
 "compilerOptions": {
  "checkJs": false
 },
 "fmt": {
  "files": {
   "include": ["."]
  },
  "options": {
   "useTabs": false,
   "lineWidth": 2000,
   "indentWidth": 1,
   "singleQuote": false,
   "proseWrap": "preserve"
  }
 }
}
